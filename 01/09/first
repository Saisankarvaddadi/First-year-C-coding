#include<stdio.h>
 int sankar( int x);
void main()
{
    int n,value;
    printf("enter the value of n");
    scanf("%d",&n);
    value=sankar(n);
    if(value==1)
    {
        printf("odd");
    }
    else
    {
        printf("even");
    }
}
 int sankar( int x)
{
    if(x%2==0)
    return 0;
    else
    return 1;
}
