#include<stdio.h>
int main()
{
    int sai[100][100],i,j,n;
    printf("emter any value");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            sai[i][j]=i*j;
        }
    }
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            printf("%d\t",sai[i][j]);
        }
        printf("\n");
    }

    return 0;
}
